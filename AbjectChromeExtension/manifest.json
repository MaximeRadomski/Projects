{
  "manifest_version": 2,

  "name": "Abject",
  "description": "I have a problem.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "http://www.google.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Le Kiwi Vaincra"
  },
  "content_scripts": [
    {
      "matches": [
            "http://*/*",
            "https://*/*"
            ],
      "js": ["sexyAssistantScript.js"],
      "run_at": "document_end"
    }
  ]
}
